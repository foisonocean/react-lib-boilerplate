{
  "name": "react-lib-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate to build React.js library using Rollup.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "non-module": "browser/react-my-lib.min.js",
  "files": [
    "lib",
    "es",
    "browser"
  ],
  "scripts": {
    "start": "run-p watch example",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "example": "parcel examples/public/index.html",
    "watch": "rollup -w -c rollup.config.js",
    "test": "jest --watch",
    "test:all": "jest",
    "prepublishOnly": "run-s build"
  },
  "bugs": {
    "url": "https://github.com/foisonocean/react-lib-boilerplate/issues"
  },
  "homepage": "https://github.com/foisonocean/react-lib-boilerplate#readme",
  "repository": "https://github.com/foisonocean/react-lib-boilerplate.git",
  "author": "foisonocean",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.4",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.2.0",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rollup": "^0.62.0",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-typescript2": "^0.15.1",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  }
}
